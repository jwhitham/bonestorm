source ../venv/bin/activate
export BONESTORM=$PWD/..
export BONESTORM_VERSION=0.1.7
export P4A_STORAGE="$BONESTORM/p4a-storage"
export P4A_PACKAGES="$P4A_STORAGE/packages"
